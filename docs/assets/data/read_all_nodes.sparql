PREFIX gsn:   <https://w3id.org/OntoGSN/ontology#>
PREFIX schema:<https://schema.org/>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?Node
WHERE {
    ?nodeIRI a gsn:GSNElement ;
             schema:identifier ?nodeID ;
             gsn:statement ?nodeStatement .
    BIND(CONCAT(STR(?nodeID), ": ", STR(?nodeStatement)) AS ?Node)
    }